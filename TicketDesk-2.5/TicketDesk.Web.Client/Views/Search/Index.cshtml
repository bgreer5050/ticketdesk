@model IEnumerable<TicketDesk.Domain.Model.Ticket>

@section title{
    Ticket Search
}

<h2>Ticket Search</h2>
@foreach (var item in Model)
{
    <article>
        @Html.DisplayNameFor(model => model.TicketId) : @Html.DisplayFor(modelItem => item.TicketId)
        <br/>
        @Html.DisplayNameFor(model => model.Title) : @Html.DisplayFor(modelItem => item.Title)
    </article>
    <hr/>
}

  