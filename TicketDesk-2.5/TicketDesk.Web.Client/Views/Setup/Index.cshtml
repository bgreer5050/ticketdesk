            <a href="#" class="td-wizard-dot"></a>
            <div class="td-wizard-info text-center"></div>
        </div>
        <div class="col-xs-2 td-wizard-step disabled">
            <!-- active -->
            <div class="text-center td-wizard-stepnum">Step 6</div>
            <div class="progress"><div class="progress-bar"></div></div>
            <a href="#" class="td-wizard-dot"></a>
            <div class="td-wizard-info text-center"></div>
        </div>
    </div>

</main>

